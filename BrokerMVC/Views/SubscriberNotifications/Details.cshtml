@model BrokerMVC.Models.SubscriberNotification


<div class="panel panel-primary">
    <div class="panel-heading">
        <div class="row">
            <div class="col-md-6 pull-right">
                <h2 class="btn btn-lg">
                    <i class="fa fa-bell"></i>
                    @Model.Title
                    @*@General.Notifications
                    /@General.View*@
                </h2>
            </div>
            <div class="btn-group pull-left">
                <button type="button" class="btn btn-close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidiven="true">&times;</span>
                </button>
            </div>
        </div>


    </div>
    <div class="panel-body">
        <div class="row">
            <div class="pull-right col-md-2">
                @General.FullName
            </div>

            <div class="col-md-10">
                @Html.DisplayFor(model => model.ObjectName)
            </div>
        </div>
        <div class="row">
            <div class="pull-right col-md-2">
                @General.Type
            </div>

            <div class="col-md-10">
                @Commons.GetObjectType(Model.ObjectTypeID)
            </div>
        </div>
        <div class="row">
            <div class="pull-right col-md-2">
                @General.Date
            </div>

            <div class="col-md-10">
                @Model.CreatedDate.Value.ToShortDateString()
            </div>
        </div>
        <div class="row">
            <div class="pull-right col-md-2">
                @General.Message
            </div>

            <div class="col-md-10">
                @Html.Raw(Model.Description)
            </div>
            </div>
        
        
        
        </div>
    </div>
